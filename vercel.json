{
    "version": 2, 
    
    "builds": [
        { "src": "server/index.js", "use": "@vercel/node" },
        { "src": "server/processing.py", "use": "@vercel/python" }
      ],
    "routes": [
        { "handle": "filesystem" },
        { "src": "/((?!recog).)*", "dest": "server/index.js" },
        { "src": "/recog", "dest": "server/processing.py" }

      ]
}